	<queryString>
		<![CDATA[select ct.documentno as fcertificacion,
case when ct.type_of_budget = 'E' then 'Extended'
     when ct.type_of_budget = 'O' then 'Operating'
     when ct.type_of_budget = 'N' then 'Income' else 'Investing' end as fpresupuesto,
	 y.year as fejercicio,
	 ct.date_issue as ffecha,
	 ba.name as farea,
	 ct.certified_value as ftotalcertificado,
	 ct.hash_code as fcodighash,
	 cc.name as fbusquearea,
	 u1.name as flineanegocio,
	 bi.name as fpartidapres,
	 ctl.certified_value as fcertvalue,
	 ctl.committed_value as fvaluecomprometido,
	 ctl.available_balance as fsaldodisponible,
	 ctl.executed_value as fvalorexecuted,
	 CASE WHEN COALESCE(ctl.certified_value,0) <>0 THEN
	 (COALESCE(ctl.executed_value,0)/ ctl.certified_value)*100
	 ELSE
	 0
	 END as percentage,
	O.ad_org_id AS organizationid,
	UPPER(O.name) AS organizacion,
	 to_char(NOW(), 'DD-MM-YYYY HH24:MI:SS') AS fecha_actual,
	L10.label AS L10_etiqueta, L10.name AS L10_nombre, L20.label AS L20_etiqueta, L20.name AS L20_nombre
from sfb_budget_certificate ct inner join sfb_budget_cert_line ctl on ct.sfb_budget_certificate_id = ctl.sfb_budget_certificate_id
                               inner join sfb_budget_item bi on ctl.sfb_budget_item_id = bi.sfb_budget_item_id
							   inner join c_year as y on y.c_year_id = ct.c_year_id
							   inner join sfb_budget_area as ba on ba.sfb_budget_area_id = ct.sfb_budget_area_id
							   inner join c_costcenter cc on cc.c_costcenter_id = ctl.c_costcenter_id
							   inner join user1 as u1 on u1.user1_id = ctl.user1_id

	left join ad_org AS O ON O.ad_org_id=ct.ad_org_id
LEFT JOIN (c_doctype AS dt1 JOIN scsdc_signaturesperdoc AS L10
	ON (dt1.c_doctype_id=L10.c_doctype_id AND L10.isactive='Y' AND L10.line=10)) ON dt1.name=$P{DOCTYPE}
LEFT JOIN (c_doctype AS dt2 JOIN scsdc_signaturesperdoc AS L20
	ON (dt2.c_doctype_id=L20.c_doctype_id AND L20.isactive='Y' AND L20.line=20)) ON dt2.name=$P{DOCTYPE}

where ct.isactive = 'Y'
AND (ct.sfb_budget_certificate_id=$P{sfb_budget_certificate_id} OR $P{sfb_budget_certificate_id} IS NULL)
AND (ct.type_of_budget=$P{type_budget} OR $P{type_budget} IS NULL)
AND (  ct.sfb_budget_area_id = $P{Sfb_Budget_Area_ID}  OR  $P{Sfb_Budget_Area_ID}  IS NULL)
AND (  ct.documentno =  $P{CertificationNo}   OR  $P{CertificationNo}  IS NULL)
$P!{sqldatestart}
$P!{sqldateend}
order by ct.documentno, ct.date_issue desc]]>
	</queryString>
